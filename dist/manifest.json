{
    "manifest_version": 2,
    "name": "Test Extention",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [ "http://*/*", "https://*/*" ],
            "js": ["js/script.bundle.js"],
            "css": ["css/style.css"]
        }
    ],
    "icons": {
        "16": "icon_16.png"
    },
    "browser_action": {
        "default_icon": "icon_16.png",
        "default_title": "sample",
        "default_popup": "html/popup.html"
    },
    "permissions": ["tabs", "contextMenus"],
    "background": {
        "scripts": ["js/bg.bundle.js"]
    }
}
